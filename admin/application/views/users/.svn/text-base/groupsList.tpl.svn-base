<h1>Groups List</h1>

<fieldset>
    <!--<legend></legend>-->
    <table border="0" cellpadding="0" cellspacing=="0" width="250">
{foreach from=$users.groupsList item=group key=groupId}
    <tr>
        <td>{$group}</td>
        <td align="right">
            <a href="/users/gedit/id/{$groupId}/" class="ctrl">edit</a>
            <a href="/users/gdelete/id/{$groupId}/" class="ctrl">delete</a>
        </td>
    </tr>
{/foreach}
</table>
</fieldset>